<%= form_with model: @company do |form| %>
  <div>
    <%= form.label :company_name %><br>
    <%= form.text_field :company_name %>
  </div>

  <div>
    <%= form.label :gstin_number %><br>
    <%= form.text_field :gstin_number %>
  </div>

  <div>
    <%= form.label :company_address %><br>
    <%= form.text_area :company_address %>
  </div>

  <div>
    <%= form.label :company_description %><br>
    <%= form.text_area:company_description %>
  </div>

  <div>
    <%= form.label :company_contact %><br>
    <%= form.text_field :company_contact %>
  </div>


  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to "Back",:back%>